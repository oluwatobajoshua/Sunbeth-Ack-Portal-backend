{
  "version": 2,
  "functions": {
    "api/index.js": {
      "includeFiles": "node_modules/sql.js/dist/sql-wasm.wasm"
    }
  },
  "routes": [
    { "src": "/api/health", "dest": "/api/health.js" },
    { "src": "/api/diag/sqljs", "dest": "/api/diag-sqljs.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" }
  ]
}
